<?xml version="1.0" encoding="UTF-8"?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="XamarinComponents.ComponentsLine"
    x:Name="ViewLine">
    <ContentView.Content>
        <Label
            BackgroundColor="{Binding BackgroundColorLine, Source={x:Reference ViewLine}}"
            Style="{StaticResource Line}"/>
    </ContentView.Content>
</ContentView>
